#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int N;
	    cin>>N;
	    int count=0;
	    string s;
	    cin>>s;
	    for(int i=0;i<N;i++){
	        if(s[i]=='1'){
	            count++;
	        }
	    }
	    int total=count+(120-N);
	    float per=(total*100)/120;
	    if(per>=75){
	        cout<<"YES"<<endl;
	    }
	    else if(per<75){
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
