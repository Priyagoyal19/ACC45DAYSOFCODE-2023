#include <iostream>
using namespace std;

int main() {
	// your code goes here
    int t,x;
    cin>>t;
    while(t--)
    {
        cin>>x;
        if(x>1 && x<2000)
        {
            cout<<"NO"<<endl;
        }
        else
        {
            cout<<"yes"<<endl;
        }
    
    }
	return 0;
}
