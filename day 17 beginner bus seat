#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
     while(t--)
     {
         int n;
         cin>>n;
         if(n<=15)
         {
             if(n<=10)
             cout<<"lower double"<<endl;
             
             else
             cout<<"lower single"<<endl;
         }
         else
         {
         if(n<=25)
         cout<<"upper double"<<endl;
         
         else
         cout<<"upper single"<<endl;
         }
     }
	return 0;
}
